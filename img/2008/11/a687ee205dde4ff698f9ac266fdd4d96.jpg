				var dummyImg = new Image();

				dummyImg.onload = function()
				{
					var vpDimensions = getViewportDimensions();
					var scaled = get_scaled_size(this.width, this.height, 578, (vpDimensions.height - 120), false);
						
					mainImage.setAttribute('width', scaled[0]);
					mainImage.setAttribute('height', scaled[1]);
					mainImage.setStyle({
						width: scaled[0],
						height: scaled[1]
					});
						
					mainImage.setAttribute('src', url_image_path);
					var a = new Element('a', { 'href': url_image_path, 'target': '_blank' });
					a.update(mainImage);
					$('mediaDiv').update(a);
				};

				dummyImg.src = url_image_path;
			}

			function flv_init()
