	var loopTime = 5000;
	var isLoopGlobe=true;
	function wp(id,titshow,conshow,titnone,connone,isLoop){
		isLoopGlobe=false;
		var o = document.getElementById(id);
		var doLoop = o.id.split("_")[1];//开始循环的tag
		var handle;//循环的setTimeout句柄
		
		var _isIE = navigator.userAgent.indexOf("MSIE")>=0?true:false;
		var tit_DivArray_tmp = o.parentNode.getElementsByTagName("div");//tag标签数组
		var tit_DivArray=new Array();
		for(var i=0;i<tit_DivArray_tmp.length;i++){
			if(tit_DivArray_tmp[i].getAttribute("name")=="t"){

				tit_DivArray[tit_DivArray.length]=tit_DivArray_tmp[i];
			}
		}
		
		var con_DivArray_tmp;
		var con_DivArray=new Array();

		if(_isIE){
			con_DivArray_tmp = o.parentNode.nextSibling.getElementsByTagName("div");
		}else{
			//var x="";
			//for   (content   in   eval(o.parentNode))   
			 // x=x   +     ".<font   color=red>"   +   content   +   "</font>="   +   eval(o.parentNode)[content]+"<br>";   
			  //document.write(x);   
			
			con_DivArray_tmp = o.parentNode.nextSibling.nextSibling.getElementsByTagName("div");
		}
		for(var i=0;i<con_DivArray_tmp.length;i++){
			if(con_DivArray_tmp[i].getAttribute("name")=="f"){
				con_DivArray[con_DivArray.length]=con_DivArray_tmp[i];
			}
		}

		for(var i=0;i<tit_DivArray.length;i++){
		//alert(tit_DivArray[i].getAttribute("id").split("_")[1]==o.getAttribute("id").split("_")[1]);
			if(tit_DivArray[i].getAttribute("id").split("_")[1]==o.getAttribute("id").split("_")[1]){
				//tit_DivArray[i].className = titshow;
				//con_DivArray[i].className = conshow;
				tit_DivArray[i].setAttribute('class',titshow);
				tit_DivArray[i].setAttribute('className',titshow);
				con_DivArray[i].setAttribute('class',conshow);
				con_DivArray[i].setAttribute('className',conshow);
			}else{
				//tit_DivArray[i].className = titnone;
				//con_DivArray[i].className = connone;
				tit_DivArray[i].setAttribute('class',titnone);
				tit_DivArray[i].setAttribute('className',titnone);
				con_DivArray[i].setAttribute('class',connone);
				con_DivArray[i].setAttribute('className',connone);
			}
		}

		if(isLoop){
			doLoop++;
			if(doLoop>tit_DivArray.length-1 || doLoop<0){
				doLoop = 0;
			}
			id=id.split("_")[0]+"_"+doLoop;
			o.parentNode.handle=setTimeout("wp('"+ id +"','"+titshow+"','"+conshow+"','"+titnone+"','"+connone+"',"+ isLoop +")",loopTime);
		}else{
			clearTimeout(o.parentNode.handle);
		}
		
	}
	function addEvent(objId,id){
		var obj=document.getElementById(objId);
		if(window.addEventListener){ // Mozilla, Netscape, Firefox
	        obj.addEventListener('mouseover', wp(id,'sy1_t01','dis','sy1_t02','undis',false), false);
	        obj.addEventListener('mouseout', wp(id,'sy1_t01','dis','sy1_t02','undis',false), true);
	    } else { // IE
	        obj.attachEvent('onmouseover',  function(){wp(id,'sy1_t01','dis','sy1_t02','undis',false);});
	        obj.attachEvent('onmouseout',  function(){wp(id,'sy1_t01','dis','sy1_t02','undis',true);});
	    }
	}
	addEvent("sy1_0","sy1_0");
	addEvent("sy1_1","sy1_1");
	addEvent("sy1_2","sy1_2");
	addEvent("sy1_3","sy1_3");
	addEvent("ihp1_r0","sy1_0");
	addEvent("ihp1_r1","sy1_1");
	addEvent("ihp1_r2","sy1_2");
	addEvent("ihp1_r3","sy1_3");
	function doLoad(){
		if(isLoopGlobe){
			document.getElementById('foda1').parentNode.handle=setTimeout("wp('sy1_0','sy1_t01','dis','sy1_t02','undis',true)",loopTime);
		}
		//document.getElementById('con_1').parentNode.handle=setTimeout("wp('tag1_0',true)",loopTime);
		//document.getElementById('con_2').parentNode.handle=setTimeout("wp('tag2_0',true)",loopTime);
	}
